! fonts

Xft.autohint:               0
Xft.lcdfilter:              lcddefault
Xft.hintstyle:              hintfull
Xft.hinting:                1
Xft.antialias:              1
Xft.rgba:                   rgb
Xft.dpi:                    96

! cursor

Xcursor.theme:              Breeze_Obsidian

! xterm

XTerm*faceName:             Menlo
xterm*faceNameDoublesize:   Microsoft Yahei
xterm*locale:               true
xterm*utf8:                 1
xterm*faceSize:             11
xterm*letterSpace:          -1
xterm*vt100*geometry:       80x30
xterm*saveLines:            30000
xterm*loginShell:           true
xterm*charClass:            33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*termName:             xterm-256color
xterm*eightBitInput:        false
xterm*borderWidth:          0

! urxvt

URxvt.depth:                32
URxvt.font:                 xft:monofur for Powerline:size=13,xft:Menlo:size=12
!URxvt.font:                 xft:Inconsolata for Powerline:size=13:regular:antialias=true
!URxvt.boldfont:             xft:Inconsolata for Powerline:size=13:bold:antialias=true
URxvt.italicFont:           xft:monofur for Powerline:size=13:style=italic
!URxvt.bolditalicFont:       xft:Inconsolata for Powerline:size=13:bold:italic:antialias=true
URxvt.loginShell:           true
URxvt.fading:               40
URxvt.cursorBlink:          false
URxvt.perl-ext-common:      default,matcher,clipboard
URxvt.matcher.button:       1
URxvt.url-launcher:         /usr/bin/google-chrome-unstable
URxvt.pointerBlank:         true
URxvt.lineSpace:            1
URxvt*cursorUnderline:      false
URxvt*termName:             rxvt-unicode-256color
URxvt*letterSpace:          0
URxvt*geometry:             80x30
URxvt*scrollBar:            false
URxvt*scrollstyle:          rxvt
!URxvt*transparent:          true
!URxvt*shading:              50
!URxvt*blurRadius:           5
URxvt*visualBell:           false
URxvt*saveLines:            30000

