! fonts

Xft.autohint:               0
Xft.lcdfilter:              lcddefault
Xft.hintstyle:              hintslight
Xft.hinting:                1
Xft.antialias:              1
Xft.rgba:                   rgb
Xft.dpi:                    96

! cursor

Xcursor.theme:              Breeze_Obsidian

! xterm

XTerm*faceName:             Monaco
xterm*faceNameDoublesize:   Microsoft Yahei
xterm*locale:               true
xterm*utf8:                 1
xterm*faceSize:             10
xterm*letterSpace:          -1
xterm*vt100*geometry:       80x30
xterm*saveLines:            30000
xterm*loginShell:           true
xterm*charClass:            33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*termName:             xterm-256color
xterm*eightBitInput:        false
xterm*borderWidth:          0

! urxvt

URxvt.depth:                32
URxvt.font:                 xft:Monaco for Powerline:size=10,xft:Monaco:size=10
!URxvt.boldfont:             xft:Inconsolata for Powerline:size=13:bold:antialias=true
URxvt.italicFont:           xft:monofur for Powerline:size=12:style=italic
!URxvt.bolditalicFont:       xft:Inconsolata for Powerline:size=13:bold:italic:antialias=true
URxvt.loginShell:           true
URxvt.fading:               40
URxvt.cursorBlink:          false
URxvt.perl-ext-common:      default,matcher,clipboard
URxvt.matcher.button:       1
URxvt.url-launcher:         /usr/bin/google-chrome-unstable
URxvt.pointerBlank:         true
URxvt.lineSpace:            1
URxvt*cursorUnderline:      false
URxvt*termName:             rxvt-unicode-256color
URxvt*letterSpace:          0
URxvt*geometry:             80x30
URxvt*scrollBar:            false
URxvt*scrollstyle:          rxvt
!URxvt*transparent:          true
!URxvt*shading:              50
!URxvt*blurRadius:           5
URxvt*visualBell:           false
URxvt*saveLines:            30000

! Hybrid Terminal Colours. Uses the palette from Tomorrow-Night:
! https://github.com/chriskempson/tomorrow-theme/blob/master/vim/colors/Tomorrow-Night.vim
! vim: ft=xdefaults

*background: #1D1F21
*foreground: #C5C8C6
*cursorColor: #AEAFAD
! black
*color0: #282A2E
*color8: #373B41
! red
*color1: #A54242
*color9: #CC6666
! green
*color2: #8C9440
*color10: #B5BD68
! yellow
*color3: #DE935F
*color11: #F0C674
! blue
*color4: #5F819D
*color12: #81A2BE
! magenta
*color5: #85678F
*color13: #B294BB
! cyan
*color6: #5E8D87
*color14: #8ABEB7
! white
*color7: #707880
*color15: #C5C8C6

