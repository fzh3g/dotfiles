! fonts

Xft.autohint:               0
Xft.lcdfilter:              lcddefault
Xft.hintstyle:              hintslight
Xft.hinting:                1
Xft.antialias:              1
Xft.rgba:                   rgb
Xft.dpi:                    96

! cursor

Xcursor.theme:              Breeze

! xterm

XTerm*faceName:             Fantasque Sans Mono
XTerm*faceNameDoublesize:   WenQuanYi Micro Hei Mono
XTerm*locale:               true
XTerm*utf8:                 1
XTerm*faceSize:             13
XTerm*letterSpace:          -1
XTerm*vt100*geometry:       80x20
XTerm*saveLines:            30000
XTerm*loginShell:           true
XTerm*charClass:            33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
XTerm*termName:             xterm-256color
XTerm*eightBitInput:        false
XTerm*borderWidth:          0
XTerm*metaSendsEscape:      true

! uxterm

UXTerm*faceName:            Fantasque Sans Mono
UXTerm*faceNameDoublesize:  WenQuanYi Micro Hei Mono
UXTerm*locale:              true
UXTerm*utf8:                1
UXTerm*faceSize:            13
UXTerm*letterSpace:         -1
UXTerm*vt100*geometry:      80x20
UXTerm*saveLines:           30000
UXTerm*loginShell:          true
UXTerm*charClass:           33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
UXTerm*termName:            xterm-256color
UXTerm*eightBitInput:       false
UXTerm*borderWidth:         0
UXTerm*metaSendsEscape:     true

! urxvt

URxvt.depth:                32
URxvt.font:                 xft:Fantasque Sans Mono:size=13
!URxvt.boldfont:             xft:Inconsolata for Powerline:size=13:bold:antialias=true
!URxvt.italicFont:           xft:Fantasque Sans Mono:size=13:style=italic
!URxvt.bolditalicFont:       xft:Inconsolata for Powerline:size=13:bold:italic:antialias=true
URxvt.loginShell:           true
URxvt.fading:               40
URxvt.cursorBlink:          false
URxvt.perl-ext-common:      default,matcher,clipboard
URxvt.matcher.button:       1
URxvt.url-launcher:         /usr/bin/google-chrome-unstable
URxvt.pointerBlank:         true
URxvt.lineSpace:            -1
URxvt*cursorUnderline:      false
URxvt*termName:             rxvt-unicode-256color
URxvt.letterSpace:          -1
URxvt*geometry:             80x20
URxvt*scrollBar:            false
URxvt*scrollstyle:          rxvt
!URxvt*transparent:          true
!URxvt*shading:              50
!URxvt*blurRadius:           5
URxvt*visualBell:           false
URxvt*saveLines:            30000

