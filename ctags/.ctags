--recurse=yes
--tag-relative=yes
--exclude=.git
--exclude=node_modules
--exclude=log
--exclude=vendor
--exclude=*.pyc
--exclude=*.pyo
--exclude=.DS_Store
--exclude=*.min.js
--exclude=jquery.*.js
--exclude=jquery-*.js

--python-kinds=-i

--langdef=IDL
--langmap=IDL:.pro
--regex-IDL=/^pro[ \t]+([a-zA-Z0-9_:]+)/\1/p,procedure/i
--regex-IDL=/^function[ \t]+([a-zA-Z0-9_:]+)/\1/f,function/i